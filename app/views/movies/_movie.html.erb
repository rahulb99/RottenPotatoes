<div id="<%= dom_id movie %>">
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Rating</th>
        <th>Release date</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= movie.title %></td>
        <td><%= movie.rating %></td>
        <td><%= movie.release_date %></td>
      </tr>
    </tbody>
  </table>
</div>